a = (input("Число, скорее всего 5-значное"))
count = int(0)
b = a.split()
b = list(reversed(b))
while count != 5:
    b = a.split
    b = list(reversed(a))
    print("step result: ", b)
    count +=1
    print(count)
    
    
    
