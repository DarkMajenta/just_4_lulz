a = []
b = []
for i in range (int(input('Колличество элементов: '))):
	a.append(int(input('Element: ')))
print(a)
for i in range (len(a)):
	#print('O____o')
	if i%2 == 0:
		#print(a[i])
		b.append(a[i])
	else:
		#print('-',a[i])
		b.append(-a[i])
print(b)

