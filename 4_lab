a = []
b = []
c = []
i=int(0)
print('For stop press !')
a.append(input('bug firsth elem 0__o'))
while a[i] != '!':
	i+=1
	a.append(input('Следующий: '))
	#print(a)
	if a[i] == '!':
		print(b)
		break
	if (int(a[i]))%2 == 0:
		b.append(a[i])
	if a[i].endswith('0'):
		c.append(a[i])	
print("i'll break")
print('Чётные: ',b)
print('Заканчиваются на ноль: ',c)
