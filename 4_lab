from random import Random, random, randint
print("import_ok") #backtrace
n = int(20) #def_count_steps
a = [] # def_array
print(a,' empty array')
for i in range(n):
	def random_element():
		random_element=(random_element.random(163,190))
	#print(random_element,' insert this')
	a.append(random_element)
print(a)

